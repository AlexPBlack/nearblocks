{
  "name": "nearblocks-indexer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#config": "./build/src/config.js",
    "#libs/*": "./build/src/libs/*.js",
    "#services/*": "./build/src/services/*.js",
    "#jobs/*": "./build/src/jobs/*.js",
    "#ts/*": "./build/src/types/*.js"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "yarn clean && tsc -p tsconfig.release.json",
    "build-neon": "cd neon && yarn && yarn build --release",
    "postbuild": "yarn build-neon && copyfiles neon/index.node build",
    "start": "node build/src/index.js",
    "dev": "nodemon --exec \"yarn build && yarn start\" --watch src --ext ts",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --config .prettierrc --write ./src"
  },
  "dependencies": {
    "@scure/base": "1.1.1",
    "@sentry/node": "7.10.0",
    "@sentry/tracing": "7.10.0",
    "axios": "0.27.2",
    "cockatiel": "3.0.0",
    "dayjs": "1.11.5",
    "globby": "13.1.2",
    "knex": "2.1.0",
    "lodash-es": "4.17.21",
    "near-lake-framework": "1.0.6",
    "pg": "8.7.3",
    "pino": "8.1.0",
    "redis": "4.1.1",
    "stream-json": "1.7.4",
    "tslib": "2.4"
  },
  "devDependencies": {
    "@types/jsesc": "^3.0.1",
    "@types/lodash-es": "~4.17",
    "@types/node": "~16",
    "@types/stream-json": "~1.7",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "copyfiles": "~2.4.1",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "nodemon": "~2.0",
    "pino-pretty": "~8.1",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-node": "~10.8",
    "tsutils": "~3.21",
    "typescript": "~4.7"
  }
}
